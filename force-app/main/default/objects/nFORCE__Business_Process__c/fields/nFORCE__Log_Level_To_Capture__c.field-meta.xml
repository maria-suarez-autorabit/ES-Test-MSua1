    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Error</fullName>
                <default>true</default>
                <label>Error</label>
            </value>
            <value>
                <fullName>Warning</fullName>
                <default>false</default>
                <label>Warning</label>
            </value>
            <value>
                <fullName>Info</fullName>
                <default>false</default>
                <label>Info</label>
            </value>
            <value>
                <fullName>Debug</fullName>
                <default>false</default>
                <label>Debug</label>
            </value>
            <value>
                <fullName>Fine</fullName>
                <default>false</default>
                <label>Fine</label>
            </value>
            <value>
                <fullName>Finer</fullName>
                <default>false</default>
                <label>Finer</label>
            </value>
            <value>
                <fullName>Finest</fullName>
                <default>false</default>
                <label>Finest</label>
            </value>
        </valueSetDefinition>
