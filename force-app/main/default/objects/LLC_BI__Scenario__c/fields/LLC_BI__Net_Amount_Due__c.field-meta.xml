<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Net_Amount_Due__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is populated automatically via formula. It stores the total sum of money due for the purchase of a service.</description>
    <formula>IF(LLC_BI__Earnings_Credit_Allowance_Amount__c &gt; LLC_BI__Svc_Total__c , LLC_BI__Svc_Total__c - LLC_BI__Earnings_Credit_Allowance_Amount__c, LLC_BI__Net_Deficit__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Net Amount Due</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
