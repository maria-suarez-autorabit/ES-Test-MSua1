<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Difference_Total__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is populated automatically via formula. This field stores the difference between the net charge difference and the total charge difference.</description>
    <formula>LLC_BI__Total_Net_Charge_Difference__c -  LLC_BI__Total_Charge_Difference__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Difference Total</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
