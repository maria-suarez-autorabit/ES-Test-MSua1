<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Discount_Percent__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is populated automatically via formula. It stores the total discount percentage for a bill point.</description>
    <formula>IF(LLC_BI__Standard_Price__c != 0, 1 - (BLANKVALUE(LLC_BI__Exception_Price__c, LLC_BI__Standard_Price__c) / LLC_BI__Standard_Price__c), 0)</formula>
    <label>Discount (%)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>4</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
