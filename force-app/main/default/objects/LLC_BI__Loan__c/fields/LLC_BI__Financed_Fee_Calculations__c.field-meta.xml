    <fullName>LLC_BI__Financed_Fee_Calculations__c</fullName>
    <deprecated>false</deprecated>
    <description>The user populates this picklist field to indicate the action the system takes with the financed fees associated with the loan. At install, this field defaults to null for existing loans and Manual Calculation for new loans.</description>
    <inlineHelpText>Select value from this picklist to add or subtract financed fees from loan amount, or manually calculate.</inlineHelpText>
    <label>Financed Fee Calculations</label>
    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>false</trackHistory>
    <type>Picklist</type>
    <valueSet>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Manual Calculation</fullName>
                <default>true</default>
                <label>Manual Calculation</label>
            </value>
            <value>
                <fullName>Add to Loan Amount</fullName>
                <default>false</default>
                <label>Add to Loan Amount</label>
            </value>
            <value>
                <fullName>Subtract From Loan Amount</fullName>
                <default>false</default>
                <label>Subtract From Loan Amount</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
