<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>nFORCE__Display_Toggle_Effect__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is optional. The admin manually populates this field. It controls the type of animation when closing or opening a screen section in the credit memo. By default, it is slide.</description>
    <label>Display Toggle Effect</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>fade</fullName>
                <default>false</default>
                <label>fade</label>
            </value>
            <value>
                <fullName>slide</fullName>
                <default>true</default>
                <label>slide</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
