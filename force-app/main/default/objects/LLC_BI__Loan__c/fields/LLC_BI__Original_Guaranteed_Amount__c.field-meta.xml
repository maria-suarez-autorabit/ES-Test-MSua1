<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Original_Guaranteed_Amount__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is automatically populated via formula. It specifies the amount of the original loan that is being guaranteed through SBA.</description>
    <formula>LLC_BI__Amount__c *  LLC_BI__SBA_Guarantee__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Original Guaranteed Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
