<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Discount_Percent_Total__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is populated automatically. This field stores the total percentage of discounts applied to the entire performa.</description>
    <formula>IF(LLC_BI__Total_Gross_Fee__c  &gt; 0, 1 - (LLC_BI__Svc_Total__c / LLC_BI__Total_Gross_Fee__c), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Discount (%)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
